{
    "compilerOptions": {
        "baseUrl": "src",
        "isolatedModules": true,
        "lib": [ "ES2015" ],
        "module": "ES2020",
        "moduleResolution": "Node",
        "paths": {"@/*": ["src/*"]},
        "sourceMap": true,
        "strict": true,
        "target": "ES2020",
        "typeRoots": ["node_modules/@types"]
    },
    "vueCompilerOptions": {
        "target": 2.7
    }
}
